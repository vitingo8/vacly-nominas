{
  "name": "@vacly/nominas-processor",
  "version": "1.0.0",
  "description": "Procesador inteligente de nóminas con IA para extracción y análisis de datos",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "require": "./dist/api/index.js",
      "import": "./dist/api/index.mjs"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "require": "./dist/lib/index.js",
      "import": "./dist/lib/index.mjs"
    }
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run build:lib && next build",
    "build:lib": "tsc --project tsconfig.lib.json && npm run build:bundle",
    "build:bundle": "rollup -c rollup.config.mjs",
    "build:secure": "NODE_ENV=production npm run build",
    "start": "next start",
    "lint": "next lint",
    "prepublishOnly": "npm run build:lib",
    "prepare-publish": "node scripts/prepare-publish.js",
    "pack": "npm pack",
    "test": "echo 'Tests will be added'",
    "clean": "rm -rf dist/"
  },
  "keywords": [
    "nominas",
    "payroll",
    "AI",
    "OCR",
    "PDF",
    "processor",
    "claude",
    "supabase",
    "vacly"
  ],
  "author": "Vacly Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "private"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@supabase/supabase-js": "^2.49.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.49.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/uuid": "^10.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "next": "15.1.8",
    "pdf-extraction": "^1.0.2",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "voyageai": "^0.0.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-loader": "^9.2.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^9",
    "eslint-config-next": "15.1.8",
    "null-loader": "^4.0.1",
    "postcss": "^8",
    "rollup": "^4.44.0",
    "rollup-plugin-dts": "^6.2.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
